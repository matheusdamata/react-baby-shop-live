{
  "name": "react-baby-shop-live",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:web": "vite",
    "dev": "concurrently \"yarn dev:web\" \"yarn dev:server\" \"yarn dev:tailwind\"",
    "dev:server": "json-server src/database/data.json -w",
    "dev:tailwind": "tailwindcss -i ./src/css/global.css -o ./src/css/output.css --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@hookform/resolvers": "^2.9.10",
    "@radix-ui/react-navigation-menu": "^1.1.1",
    "@radix-ui/react-tabs": "^1.0.1",
    "@types/styled-components": "^5.1.26",
    "axios": "^1.1.3",
    "concurrently": "^7.6.0",
    "immer": "^9.0.16",
    "phosphor-react": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "styled-components": "^5.3.6",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^1.1.3",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  }
}
